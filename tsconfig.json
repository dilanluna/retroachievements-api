{
	"compilerOptions": {
		"target": "es2016",
		"module": "commonjs",
		"types": ["vitest/globals"],
		"declaration": true,
		"sourceMap": true,
		"outDir": "./dist",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"noImplicitAny": true,
		"skipLibCheck": true
	}
}
